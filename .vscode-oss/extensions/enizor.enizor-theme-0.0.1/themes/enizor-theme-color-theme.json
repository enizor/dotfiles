    "tokenColors": [
        {
            "name": "Comment",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                // "fontStyle": "italic",
                "foreground": "#999999" //3
            }
        },
        {
            "name": "Variables, Parameters",
            "scope": [
                "variable",
                "entity.name.variable.parameter",
                "entity.name.variable.local",
                "variable.parameter"
            ],
            "settings": {
                "foreground": "#339999" //8
            }
        },
        {
            "name": "Properties",
            "scope": [
                "variable.other.object.property"
            ],
            "settings": {
                "foreground": "#6666CC" //D
            }
        },
        {
            "name": "Colors",
            "scope": [
                "constant.other.color"
            ],
            "settings": {
                "foreground": "#CC6633" //B
            }
        },
        {
            "name": "Invalid",
            "scope": [
                "invalid",
                "invalid.illegal"
            ],
            "settings": {
                "foreground": "#CC3333" //8
            }
        },
        {
            "name": "Invalid - Deprecated",
            "scope": [
                "invalid.deprecated"
            ],
            "settings": {
                "foreground": "#CC6633" //F
            }
        },
        {
            "name": "Keyword, Storage",
            "scope": [
                "support.class",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#6666CC" //E
            }
        },
        {
            "name": "Keyword Control",
            "scope": [
                "keyword",
                "keyword.control",
                "keyword.control.flow",
                "keyword.control.from",
                "keyword.control.import",
				"keyword.other.unit",
                "keyword.other",
				"keyword.control.as"
            ],
            "settings": {
                "foreground": "#CCCC33" //E
            }
        },
        {
            "name": "Keyword",
            "scope": [
                "keyword.type",
                "keyword.other.using",
                "keyword.other.namespace",
                "keyword.other.class",
                "keyword.other.new",
                "keyword.other.event",
                "keyword.other.this",
                "keyword.other.await",
                "keyword.other.var",
                "keyword.other.package",
                "keyword.other.import",
                "variable.language.this",
                "storage.type.ts"
            ],
            "settings": {
                "foreground": "#996699" //E
            }
        },
        {
            "name": "Types, Primitives",
            "scope": [
                "entity.name"
            ],
            "settings": {
                "foreground": "#339933" //C
            }
        },
        {
            "name": "Function",
            "scope": [
                "storage.type.function"
            ],
            "settings": {
                "foreground": "#CC6633" //D
            }
        },
        {
            "name": "Operator, Misc",
            "scope": [
                "punctuation",
                "punctuation.section.class.end",
                "meta.tag",
                "punctuation.definition.tag",
                "punctuation.separator.inheritance.php",
                "punctuation.definition.tag.html",
                "punctuation.definition.tag.begin.html",
                "punctuation.definition.tag.end.html",
                "keyword.other.template",
                "keyword.other.substitution"
            ],
            "settings": {
                "foreground": "#CCCCCC" //5
            }
